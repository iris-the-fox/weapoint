<p id="notice"><%= notice %></p>

<h1>Learnings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Note</th>
      <th>Tag</th>
      <th>Range</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @learnings.each do |learning| %>
      <tr>
        <td><%= learning.name %></td>
        <td><%= learning.note %></td>
        <td><%= learning.tag %></td>
        <td><%= learning.range %></td>
        <td><%= link_to 'Show', learning %></td>
        <% if logged_in? %>
        <td><%= link_to 'Edit', edit_learning_path(learning) %></td>
        <td><%= link_to 'Destroy', learning, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>


<% if logged_in? %>
<%= link_to 'New Learning', new_learning_path %>
<% end %>